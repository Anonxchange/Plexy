{
  "rewrites": [
    {
      "source": "/",
      "destination": "/support",
      "has": [
        {
          "type": "host",
          "value": "help.pexly.app"
        }
      ]
    },
    {
      "source": "/",
      "destination": "/signin",
      "has": [
        {
          "type": "host",
          "value": "auth.pexly.app"
        }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*\\.html|index\\.html)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
