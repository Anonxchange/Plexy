{
  "rewrites": [
    {
      "source": "/",
      "destination": "/support",
      "has": [
        {
          "type": "host",
          "value": "help.pexly.app"
        }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}
